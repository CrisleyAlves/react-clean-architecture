{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react",
    "rootDir": "src",
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"]
    },
    "allowJs": true,
    "resolveJsonModule": true,
    // "skipDefaultLibCheck": true,
    // "allowSyntheticDefaultImports": true,
    // "strict": true,
    // "forceConsistentCasingInFileNames": true,
    // "isolatedModules": true,
    // "noEmit": true
  },
  "include": [
    "src"
  ]
}